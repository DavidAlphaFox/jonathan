{
  "author": "Rudolph-Miller",
  "name": "react-jonathan",
  "version": "0.0.2",
  "keywords": [
    "react",
    "react-component",
    "mixins",
    "jonathan"
  ],
  "description": "React mixins for Jonathan",
  "main": "index.js",
  "scripts": {
    "install": "jsx --extension jsx --no-cache-dir src/ js/",
