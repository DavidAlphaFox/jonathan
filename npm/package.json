    "type": "git",
    "url": "git://github.com/Rudolph-Miller/jonathan/tree/master/npm"
  },
  "dependencies": {
    "jsx-loader": "^0.12.2",
    "qwest": "^1.5.4",
    "react": "^0.12.2"
  },
